                            if (nextTabLinkEl.innerHTML == 'Contingent Resources') {
                                const nextDoubleTabLinkEl = $('.nav-tabs .active').closest('li').next('li').next('li').find('a')[0];
                                const nextTab = new bootstrap.Tab(nextDoubleTabLinkEl);
